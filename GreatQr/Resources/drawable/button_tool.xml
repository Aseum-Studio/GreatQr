<?xml version="1.0" encoding="UTF-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#eb9c1e" android:startColor="#EB9C6F" android:type="linear" />
    </shape>
  </item>
  <item android:state_pressed="true">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#4DEB9C1E" android:startColor="#4dEB9C6F" android:type="linear" />
    </shape>
  </item>
</selector>